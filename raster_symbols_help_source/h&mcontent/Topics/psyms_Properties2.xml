<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../helpproject.xsl" ?>
<topic template="Default" lasteditedby="Andre" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../helpproject.xsd">
  <title translate="true">Properties</title>
  <body>
    <para styleclass="Normal"><text styleclass="Normal" translate="true">The statistics_engine has the following properties:</text></para>
    <para styleclass="Normal"><table styleclass="Default" rowcount="20" colcount="2" style="border-width:0px; border-spacing:0px; border-collapse:collapse; cell-border-width:1px; border-color:#808080; border-style:solid; head-row-background-color:#d0d0d0; alt-row-background-color:none;">
      <thead style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Name</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="Normal"><text styleclass="Normal" translate="true">Description</text></para>
        </td>
      </thead>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">rwo_type</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">The rwo_type/collection name</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">ds_name</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">The name of the dataset for rwo_type</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">value_method</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">Method/Fieldname</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">value_list</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">value_type</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">Type of value</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">number_of_objects</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">Total number of objects evaluated</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">non_responsive</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">Number of objects that did not respond to the value_method</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">num_unset_values</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">Number of objects without a value</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">num_negative_values</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">Number of negative values</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">errors</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">rwo_types</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">List of rwo_types if the statistics_engine operates on a heterogeneous set of objects</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px; height:12px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">min_value</text></para>
        </td>
        <td style="width:897px; height:12px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">The lowest value found</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">max_value</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">The highest value found</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">range</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">The range (max_value-min_value)</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">sum</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">The sum of values</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">average</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">The average value</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">scale</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">The measurement scale in which values can be presented, </text></para>
          <para styleclass="TableText"><text styleclass="TableText" translate="true">either calculated (user_scale? is _false) or set by user (user_scale? is _false)</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">user_scale?</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">True if .scale is set manually by method set_scale()</text></para>
        </td>
      </tr>
      <tr style="vertical-align:top">
        <td style="width:155px;">
          <para styleclass="TableText"><text styleclass="Normal" style="font-weight:bold; color:#000000;" translate="true">actual_value_manager</text></para>
        </td>
        <td style="width:897px;">
          <para styleclass="TableText"><text styleclass="TableText" translate="true">reserved</text></para>
        </td>
      </tr>
    </table></para>
  </body>
</topic>
